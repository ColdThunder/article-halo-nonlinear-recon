	Date-Added = {2015-01-20 01:47:07 +0000},
	Date-Modified = {2015-08-21 02:59:53 +0000},
	Doi = {10.1088/1475-7516/2013/11/048},
	Eid = {048},
	Eprint = {1305.4642},
	Journal = {\bibjcap},
	Month = nov,
	Pages = {48},
	Primaryclass = {astro-ph.CO},
	Title = {{One-point remapping of Lagrangian perturbation theory in the mildly non-linear regime of cosmic structure formation}},
	Volume = 11,
	Year = 2013,
