	Doi = {10.1088/0004-637X/798/1/40},
	Eid = {40},
	Eprint = {1410.7794},
	Journal = {\bibapj},
	Keywords = {cosmological parameters, large-scale structure of universe},
	Month = jan,
	Pages = {40},
	Title = {{Forecasts on the Dark Energy and Primordial Non-Gaussianity Observations with the Tianlai Cylinder Array}},
	Volume = 798,
	Year = 2015,
	Bdsk-File-1 = {},
