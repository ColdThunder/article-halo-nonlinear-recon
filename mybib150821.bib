	Eprint = {astro-ph/9508025},
	Journal = {\bibapj},
	Keywords = {COSMOLOGY: THEORY, COSMOLOGY: DARK MATTER, GALAXIES: HALOS, METHODS: NUMERICAL},
	Month = may,
	Pages = {563},
	Title = {{The Structure of Cold Dark Matter Halos}},
