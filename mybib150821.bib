	Eprint = {1310.6038},
	Journal = {\bibmnras},
	Keywords = {methods: statistical, cosmology: theory, large-scale structure of Universe},
	Month = mar,
	Pages = {L11-L15},
	Title = {{Sufficient observables for large-scale structure in galaxy surveys}},
	Volume = 439,
	Year = 2014,
