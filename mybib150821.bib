	Date-Modified = {2015-08-21 02:47:26 +0000},
	Doi = {10.1103/PhysRevD.91.043523},
	Eid = {043523},
	Eprint = {1409.6809},
	Journal = {\bibprd},
	Keywords = {Cosmology, Dark energy, Origin and formation of the Universe},
