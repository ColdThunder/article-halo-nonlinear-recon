	Author = {{Padmanabhan}, N. and {Xu}, X. and {Eisenstein}, D.~J. and {Scalzo}, R. and {Cuesta}, A.~J. and {Mehta}, K.~T. and {Kazin}, E.},
	Date-Added = {2016-12-07 01:13:33 +0000},
	Date-Modified = {2016-12-07 01:13:43 +0000},
	Doi = {10.1111/j.1365-2966.2012.21888.x},
