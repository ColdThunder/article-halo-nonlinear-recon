	Eprint = {1411.4117},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics},
	Month = nov,
	Title = {{On the Nonlinear Evolution of Cosmic Web: Lagrangian Dynamics Revisited}},
	Year = 2014,
	Bdsk-File-1 = {},
