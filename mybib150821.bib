	Eprint = {astro-ph/9609122},
	Journal = {\bibaap},
	Keywords = {COSMOLOGY: DARK MATTER, LARGE-SCALE STRUCTURES, GRAVITATIONAL LENSING},
	Month = jun,
	Pages = {1-18},
	Title = {{Weak lensing statistics as a probe of $\{$OMEGA$\}$ and power spectrum.}},
