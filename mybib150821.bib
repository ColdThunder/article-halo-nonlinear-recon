	Author = {{Joyce}, A. and {Jain}, B. and {Khoury}, J. and {Trodden}, M.},
	Date-Added = {2015-04-28 02:32:52 +0000},
	Date-Modified = {2015-08-21 02:59:53 +0000},
	Doi = {10.1016/j.physrep.2014.12.002},
	Eprint = {1407.0059},
	Journal = {\bibphysrep},
	Keywords = {Cosmology, Modified gravity, Screening mechanisms, Dark energy},
	Month = mar,
	Pages = {1-98},
