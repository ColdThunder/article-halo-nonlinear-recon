	Journal = {\bibmnras},
	Keywords = {gravitational lensing, cosmology: theory, large-scale structure of Universe},
	Month = sep,
	Pages = {857-886},
	Title = {{Three-point correlations in weak lensing surveys: model predictions and applications}},
	Volume = 344,
	Year = 2003,
