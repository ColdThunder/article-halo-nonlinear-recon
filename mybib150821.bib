	Doi = {10.1111/j.1365-2966.2011.19108.x},
	Eprint = {1103.3245},
	Journal = {\bibmnras},
	Keywords = {gravitational lensing: weak, methods: numerical, methods: statistical, large-scale structure of Universe},
	Month = sep,
	Pages = {1045-1055},
	Title = {{Simulating the effect of non-linear mode coupling in cosmological parameter estimation}},
